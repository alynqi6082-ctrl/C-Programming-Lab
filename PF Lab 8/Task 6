#include <stdio.h>
int main() {
    int a[3][3], t[3][3],i,j;
     printf("Enter element of the 3x3 Matix:\n ");
    for( i=0;i<3;i++)
        for( j=0;j<3;j++)
            scanf("%d\n", &a[i][j]);

    for( i=0;i<3;i++)
        for( j=0;j<3;j++)
            t[j][i] = a[i][j];

    printf("\nOriginal Matrix:\n");
    for( i=0;i<3;i++){
        for( j=0;j<3;j++)
            printf("%d ", a[i][j]);
       	 printf("\n");
    }

    printf("\nTransposed Matrix:\n");
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++)
            printf("%d ", t[i][j]);
         printf("\n");
    }
    return 0;
}

