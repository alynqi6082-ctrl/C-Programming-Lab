#include <stdio.h>

int main() {
    char n[3][20];
    
    for(int i=0;i<3;i++)
        scanf("%s", n[i]);
    
	for(int i=0;i<3;i++) {
        printf("Characters in Name %d:\n", i+1);
        for(int j=0;n[i][j]!='\0';j++)
           
	 	printf("%c ", n[i][j]);
        printf("\n");
    }
    return 0;
}

