#include <stdio.h>
int main() {
	
    int arr1[5], arr2[5], merged[10], i;
    
	printf("Enter 5 integers for array 1:\n");
    for (i = 0; i < 5; i++) 
	{
	scanf("%d", &arr1[i]);
	}
	
	printf("Enter 5 integers for array 2:\n");
    for (i = 0; i < 5; i++) 
	{
	scanf("%d", &arr2[i]);
	}
    for (i = 0; i < 5; i++) 
	merged[i] = arr1[i];
    
	for (i = 0; i < 5; i++) 
	merged[5 + i] = arr2[i];
    
	printf("Merged Array: ");
    for (i = 0; i < 10; i++) 
	printf("%d ", merged[i]);
    
	
    return 0;
}
