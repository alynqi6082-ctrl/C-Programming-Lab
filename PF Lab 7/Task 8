#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main() {
    char str[100];
    int i;
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);
    for (i = 0; str[i] != '\0'; i++)
        if (strchr("aeiou", tolower(str[i]))) 
	str[i] = toupper(str[i]);
    printf("Modified String: %s", str);
    return 0;
}
