#include <stdio.h>
int main() {
    int grades[10] = {65, 70, 80, 90, 50, 75, 85, 60, 95, 55};
    int i, sum = 0, max = grades[0], min = grades[0], max_i = 0, min_i = 0;
    for (i = 0; i < 10; i++) {
        printf("Student %d: %d\n", i + 1, grades[i]);
        sum += grades[i];
        
        if (grades[i] > max) { 
        
		max = grades[i];
		max_i = i; 
	}
        if (grades[i] < min) {
		 
		min = grades[i]; 
		min_i = i; 
	}
    }
    
    printf("Average: %.2f\n", sum / 10.0);
    printf("Highest: %d  (Index: %d)\nLowest: %d  (Index: %d)\n", max, max_i, min, min_i);
    grades[min_i] = min + 10;
    printf("\nUpdated Grades: ");
    
    for (i = 0; i < 10; i++) 
	printf("%d ", grades[i]);
    printf("\n");
    
    return 0;
}
